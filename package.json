{
  "name": "dimension-trails",
  "version": "0.0.1",
  "description": "Orbital motion through 01234D & time",
  "engines": {
    "node": "11.0.0",
    "npm": "6.4.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./docs && parcel build --out-dir ./docs --public-url ./ ./src/index.html",
    "dev": "rm -rf ./dev && parcel --out-dir dev ./src/index.html",
    "tsc": "tsc --watch --noEmit",
    "prettier": "prettier --write './src/**/*.+(json|md|scss|ts|tsx)'",
    "tslint": "tslint --fix -p ./tsconfig.json './src/**/*.+(ts|tsx)'",
    "format": "npm run prettier && npm run tslint",
    "clean": "rm -rf .cache && rm -rf ./dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexburner/dimension-trails.git"
  },
  "keywords": [
    "multidimensional",
    "dimensions",
    "0D",
    "1D",
    "2D",
    "3D",
    "4D",
    "space",
    "time",
    "orbits",
    "motion",
    "spread",
    "trails",
    "emergence",
    "complexity"
  ],
  "author": "aburner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexburner/dimension-trails/issues"
  },
  "homepage": "https://github.com/alexburner/dimension-trails#readme",
  "devDependencies": {
    "@types/three": "0.93.4",
    "parcel-bundler": "1.10.3",
    "prettier": "1.14.3",
    "sass": "1.14.3",
    "tslint": "5.11.0",
    "tslint-consistent-codestyle": "1.13.3",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript": "3.1.3"
  },
  "dependencies": {
    "three": "0.97.0"
  }
}
